{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UseTracingExporter": "zipkin",
  "UseMetricsExporter": "otlp",
  "UseLogExporter": "otlp",
  "Jaeger": {
    "ServiceName": "client-jaeger-test",
    "AgentHost": "localhost",
    "AgentPort": 6831,
    "Endpoint": "http://localhost:14268",
    "Protocol": "UdpCompactThrift"
  },
  "Prometheus": {
    "ScrapeResponseCacheDurationMilliseconds": 5000
  },
  "Zipkin": {
    "ServiceName": "client-zipkin-test",
    "Endpoint": "http://localhost:9411/api/v2/spans"
  },
  "Otlp": {
    "ServiceName": "client-otlp-test",
    "Endpoint": "http://localhost:4318"
  },
  "AspNetCoreInstrumentation": {
    "RecordException": "true"
  }
}
